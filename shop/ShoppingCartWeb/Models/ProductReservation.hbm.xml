<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
				   assembly="ShoppingCartWeb"
				   namespace="ShoppingCartWeb.Models">
	<class name="ProductReservation"
		   table="ProductReservation">
		<id name="Id">
			<generator class="hilo"/>
		</id>
    <property name="Count" />
    
    <set name="RegularBuyers"
         cascade="all-delete-orphan"
         batch-size="5"
         >
      <key column="RegularBuyerResId" />
      <one-to-many class="Customer" />
    </set>
    <many-to-one name="Product"
                 class="ProductItem"
                 column="ProductId"
                 cascade="save-update" />
    
		<!--<many-to-one name="Cart"
                 class="ShoppingCart"
                 column="CartId"
                 cascade="save-update" />-->
	</class>
</hibernate-mapping>