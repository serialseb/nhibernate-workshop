<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping 
  xmlns="urn:nhibernate-mapping-2.2"
				   assembly="Ex-App1"
				   namespace="Eq1.App1.Model">
  <class name="Player"
		   table="Players">

    <id name="Id">
      <generator class="hilo"/>
    </id>
    <property name="FullName"/>
    <property name="Salary" />
    <many-to-one 
      class="Team" 
      name="Team" 
      column="TeamId" />
  </class>
</hibernate-mapping>