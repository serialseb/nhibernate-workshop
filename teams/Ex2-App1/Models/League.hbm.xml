<hibernate-mapping 
  xmlns="urn:nhibernate-mapping-2.2"
				   assembly="Ex-App1"
				   namespace="Eq1.App1.Model">
  <class name="League"
		   table="Leagues">

    <id name="Id">
      <generator class="identity"/>
    </id>
    <property name="Name"/>

    <set name="Teams" cascade="save-update"
			 table="Teams" batch-size="999" lazy="false">

      <key column ="LeagueId"/>
